import { h, FunctionalComponent } from 'preact';
import {
    $nav,
    $right,
    $something,
    $something2
} from './styles.css';
interface Props {
  prev: PaginationData;
  next: PaginationData;
}
const TestPagination: FunctionalComponent<Props> = ({ prev, next }: Props) => {
  return (
    <div class={$nav}>
      <a href={prev.link}>
        <svg class={$something} viewBox="0 0 9 5">
          <path d="M4.6 2.7L9 0v2.1L4.7 5.3h-.2L0 2.1V0z" />
        </svg>
        <span>{prev.title}</span>
      </a>
      <a href={next.link}>
          <span class={$right}>{next.title}</span>
        <svg class={$something2} viewBox="0 0 9 5">
          <path d="M4.6 2.7L9 0v2.1L4.7 5.3h-.2L0 2.1V0z" />
        </svg>
      </a>
    </div>
  );
};

export default TestPagination;
